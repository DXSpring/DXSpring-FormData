  "bugs": {
    "url": "https://github.com/DXSpring/DXSpring-FormData/issues"
  },
  "homepage": "https://github.com/DXSpring/DXSpring-FormData#readme",
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
